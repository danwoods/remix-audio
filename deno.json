{
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": ["deno.window", "dom", "dom.iterable", "esnext"],
    "strict": true,
    "skipLibCheck": true
  },
  "imports": {
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.614.0",
    "@aws-sdk/credential-providers": "npm:@aws-sdk/credential-providers@^3.614.0",
    "music-metadata": "npm:music-metadata@^10.0.0",
    "music-metadata-browser": "npm:music-metadata-browser@^2.5.11",
    "id3js": "npm:id3js@^2.1.1",
    "extract-colors": "npm:extract-colors@^4.0.6",
    "match-sorter": "npm:match-sorter@^6.3.1",
    "sort-by": "npm:sort-by@^1.2.0",
    "tiny-invariant": "npm:tiny-invariant@^1.3.1",
    "@std/assert": "https://deno.land/std@0.208.0/assert/mod.ts"
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --allow-write --allow-sys --watch server/main.ts",
    "start": "deno run --allow-net --allow-env --allow-read --allow-write --allow-sys server/main.ts",
    "build": "deno task build:custom-elements",
    "build:docs": "deno doc --html --name='Remix Audio Components' --output=./docs doc.exports.ts",
    "build:custom-elements": "deno bundle --minify app/components/register-custom-elements.ts > build/main.js",
    "test:browser": "deno run --allow-net --allow-env --allow-read --allow-run --allow-sys --allow-write --no-check deno-tests/server/handlers/album.html.sticky.browser.playwright.ts",
    "test:doc": "deno test --doc --allow-net app/util/track.ts",
    "test:all": "deno task test:doc && deno test deno-tests/ 'app/components/AlbumImage/**/*.test.ts' 'app/components/Layout/**/*.test.ts' 'app/components/ScrollingText/**/*.test.ts' 'app/components/Tracklist/**/*.test.ts' 'app/components/UploadDialog/**/*.test.ts' --no-check --allow-net --allow-env --allow-read --allow-write --allow-sys"
  },
  "fmt": {
    "include": ["app/", "server/"]
  },
  "lint": {
    "include": ["server/", "app/"],
    "rules": {
      "tags": ["recommended"]
    }
  },
  "name": "BoomBox"
}
