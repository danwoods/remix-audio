{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["deno.window", "dom", "dom.iterable", "esnext"],
    "strict": true,
    "skipLibCheck": true
  },
  "imports": {
    "react": "npm:react@^18.2.0",
    "react-dom": "npm:react-dom@^18.2.0",
    "react-dom/server": "npm:react-dom@^18.2.0/server",
    "react-dom/client": "npm:react-dom@^18.2.0/client",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.614.0",
    "@aws-sdk/credential-providers": "npm:@aws-sdk/credential-providers@^3.614.0",
    "@heroicons/react": "npm:@heroicons/react@^2.1.5",
    "music-metadata": "npm:music-metadata@^10.0.0",
    "music-metadata-browser": "npm:music-metadata-browser@^2.5.11",
    "id3js": "npm:id3js@^2.1.1",
    "extract-colors": "npm:extract-colors@^4.0.6",
    "react-intersection-observer": "npm:react-intersection-observer@^9.13.0",
    "match-sorter": "npm:match-sorter@^6.3.1",
    "sort-by": "npm:sort-by@^1.2.0",
    "tiny-invariant": "npm:tiny-invariant@^1.3.1",
    "@std/assert": "https://deno.land/std@0.208.0/assert/mod.ts"
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --allow-write --allow-sys --watch server/main.ts",
    "start": "deno run --allow-net --allow-env --allow-read --allow-write --allow-sys server/main.ts",
    "lint": "deno lint server/ deno-tests/",
    "lint:fix": "deno lint --fix server/ deno-tests/"
  },
  "fmt": {
    "include": ["app/", "server/"]
  },
  "lint": {
    "include": ["server/", "deno-tests/"],
    "rules": {
      "tags": ["recommended"]
    }
  },
  "name": "Stranger Sounds"
}
